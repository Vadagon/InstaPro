{
  "name": "InstaPro",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "InstaPro",
  "icons": {
    "16": "chrome/icons/icon16.png",
    "48": "chrome/icons/icon48.png",
    "128": "chrome/icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "chrome/src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "chrome/icons/icon19.png",
    "default_title": "browser action demo"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "https://www.instagram.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "css": [
        "chrome/src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "chrome/src/inject/inject.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
