{
  "name": "App Assistant",
  "version": "0.9.8",
  "manifest_version": 2,
  "description": "App Assistant For Instagramâ„¢",
  "icons": {
    "16": "chrome/icons/icon16.png",
    "48": "chrome/icons/icon48.png",
    "128": "chrome/icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "chrome/src/bg/jquery.min.js",
      "chrome/src/bg/utils.js",
      "chrome/src/bg/headers.js",
      "chrome/src/bg/main.js",
      "chrome/src/bg/tools.js",
      "chrome/src/bg/background.js",
      "chrome/src/bg/buy.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "chrome/icons/icon19.png",
    "default_title": "browser action demo"
  },
  "permissions": [
    "alarms",
    "background",
    "power",
    "storage",
    "webRequest",
    "identity",
    "identity.email",
    "unlimitedStorage",
    "webRequestBlocking",
    "cookies",
    "activeTab",
    "https://www.instagram.com/*",
    "<all_urls>",
    "debugger",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": [
        "chrome/src/bg/jquery.min.js",
        "chrome/src/inject/inject.js"
      ],
      "css": ["chrome/src/inject/inject.css"],
      "all_frames": true
    }
  ],
  "web_accessible_resources": ["/*"]
}
